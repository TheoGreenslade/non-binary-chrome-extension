{
    "manifest_version": 3,
    "name": "Non binary word replacer",
    "description": "Replacing gendered words with their non-binary equivalent",
    "version": "1.0",
    "author": "Theo Greenslade",
    "action": {
      "default_popup": "hello.html",
      "default_icon": "hello_extensions.png"
    },
    "content_scripts":
    [
        {
            "matches": ["*://*/*"],
            "all_frames": true,
            "js": ["content_script.js"],
            "run_at": "document_end"
        }
    ]
  }
  